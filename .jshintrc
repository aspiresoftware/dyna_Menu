{
  "node": true,
  "browser": true,
  "esnext": true,
  "bitwise": true,
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "immed": true,
  "freeze": true,
  "indent": 2,
  "latedef": "nofunc",
  "maxcomplexity": 10,
  "maxlen": 100,
  "newcap": true,
  "noarg": true,
  "noempty": true,
  "nonew": true,
  "nonbsp": true,
  "plusplus": true,
  "quotmark": "single",
  "regexp": true,
  "undef": true,
  "unused": true,
  "strict": false,
  "trailing": true,
  "smarttabs": true,
  "globals": {
    // angular
    "angular": false,

    // lodash
    "_": false,

    "$": false,

    // angular-mocks
    "inject": false,

    // Jasmine
    "beforeEach": false,
    "afterEach": false,
    "describe": false,
    "expect": false,
    "it": false,
    "spyOn": false,
    "xdescribe": false,
    "xit": false
  }
}
